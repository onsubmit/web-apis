{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "paths": {
      "@components/*": ["src/components/*"],
      "@snippets/*": ["src/content/snippets/*"]
    }
  }
}
